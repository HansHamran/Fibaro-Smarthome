--[[
%% autostart
%% properties
700 value
706 value
699 value
705 value
%% events
%% globals
--]]

local startSource = fibaro:getSourceTrigger();
if (
 ( tonumber(fibaro:getValue(700, "value")) < 200  
      and tonumber(fibaro:getValue(706, "value")) < 200
      and tonumber(fibaro:getValue(705, "value")) < 31 
      and  tonumber(fibaro:getValue(699, "value")) < 31 )
or
startSource["type"] == "other"
)
then
	if
    fibaro:getGlobalValue("Utelys") == "Off" 
    then
    fibaro:setGlobal("Utelys", "On");
    end
end
