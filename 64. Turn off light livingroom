	fibaro:call(51, "turnOff");
	fibaro:call(59, "turnOff");
	fibaro:call(67, "turnOff");
	fibaro:call(71, "turnOff");
	fibaro:call(63, "turnOff");
	fibaro:call(267, "turnOff");
	fibaro:call(512, "turnOff");
end


